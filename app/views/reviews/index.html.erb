<h1>My reviews</h1>
<% @reviews.each do |review| %>
<div class="image-box">
  <img src="<%= cl_image_path review.game.photo.key %>" alt="<%= review.game.title %>">
  <div class="box">
  </div>
</div>
<p></p>
<p><%=review.game.title%></p>
<p><%=review.rating%> <i class="fa-solid fa-star"></i></p>
<p><%=review.content%></p>
<% end %>
